<RSISynoptic
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:HMI="clr-namespace:UGP_STG.HMI" x:Class="UGP_STG.LOCAL_PANEL.LOCAL_PANEL3"
          Background="LightGray"
          Height="1048"
          Width="1920"
          mc:Ignorable="d">
    <Grid>
        <!--  GRAPHIC Part  -->
        <Canvas x:Name="GRAPHIC">
            <TextBlock
                FontSize="72"
                Text="LOCAL_PANEL2" Canvas.Left="704.48" Canvas.Top="10" FontWeight="Bold" />
            <TextBlock
                FontSize="48"
                Text="PROTECTION TRIP FROM LINE PROTECTION A / B" Canvas.Left="177.605" Canvas.Top="321.397" FontWeight="Bold" />
            <TextBlock
                FontSize="24"
                Text="HSH2889B" Canvas.Left="1469.773" Canvas.Top="245.429" FontWeight="Bold" Width="134.227" />
            <TextBlock
                FontSize="24"
                Text="HSH2889C" Canvas.Left="1469.773" Canvas.Top="386.429" FontWeight="Bold" Width="134.227" />
            <TextBlock
                FontSize="48"
                Text="GENERATOR PROTECTION TRIP FROM RELAY B" Canvas.Left="177.605" Canvas.Top="184.632" FontWeight="Bold" />
            <TextBlock
                FontSize="48"
                Text="EMERGENCY STOP BUTTON NOISEHOOD 3" Canvas.Left="177.605" Canvas.Top="443.397" FontWeight="Bold" />
            <TextBlock
                FontSize="24"
                Text="HSH2908" Canvas.Left="1469.773" Canvas.Top="515.929" FontWeight="Bold" Width="134.227" />
        </Canvas>

        <!--  FOD Part  -->
        <Canvas x:Name="FOD"
                Visibility="{FODBinding}">
            <RSI_StdPushButton Canvas.Left="1495.774" Canvas.Top="178.683" ModelName="ZS2889B_GS_NOTRIP" StartText="ON" StopText="OFF"/>
            <RSI_StdPushButton Canvas.Left="1495.774" Canvas.Top="313.269" ModelName="ZS2889C_GS_NOTRIP" StartText="ON" StopText="OFF"/>


            <HMI:Navigaton x:Name="EmbeddedSymbol57"
        Canvas.Left="1810.501"
        Canvas.Top="1041.359"
        Width="29"
        Height="38.783"
        RenderTransformOrigin="0,0"
        Type="Main" DISP="LOCAL_PANEL1">
                <HMI:Navigaton.RenderTransform>
                    <ScaleTransform ScaleX="1" ScaleY="-1" />
                </HMI:Navigaton.RenderTransform>
            </HMI:Navigaton>
            <RSIText x:Name="TextBox2_1"
        Canvas.Left="1767.166"
        Canvas.Top="992.409"
        Width="47.667"
        Height="24"
        HorizontalContentAlignment="Right"
        VerticalContentAlignment="Center"
        BorderBrush="Transparent"
        Foreground="#FF000000"
        IsHitTestVisible="False"
        Text="NEXT"
        TextElement.FontFamily="Arial"
        TextElement.FontSize="10" />
            <HMI:Navigaton x:Name="EmbeddedSymbol57_Copy"
        Canvas.Left="90.497"
        Canvas.Top="1041.359"
        Width="29"
        Height="38.783"
        RenderTransformOrigin="0,0"
        Type="Main" DISP="LOCAL_PANEL2">
                <HMI:Navigaton.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleY="-1" ScaleX="-1"/>
                        <SkewTransform AngleY="0" AngleX="0"/>
                        <RotateTransform Angle="0"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Navigaton.RenderTransform>
            </HMI:Navigaton>
            <RSIText x:Name="TextBox2_1_Copy"
        Canvas.Left="67.162"
        Canvas.Top="992.409"
        Width="47.667"
        Height="24"
        HorizontalContentAlignment="Right"
        VerticalContentAlignment="Center"
        BorderBrush="Transparent"
        Foreground="#FF000000"
        IsHitTestVisible="False"
        Text="PREV"
        TextElement.FontFamily="Arial"
        TextElement.FontSize="10" />
            <RSI_StdPushButton Canvas.Left="1495.774" Canvas.Top="447.269" ModelName="HSH2908" StartText="ON" StopText="OFF"/>


        </Canvas>

        <!--  Battery Limit Part  -->
        <Canvas x:Name="BL"
                Visibility="{BLBinding}" d:IsLocked="True"/>

        <!--  FAILURE Part  -->
        <Canvas x:Name="FAILURE"
                Visibility="{FAILUREBinding}" d:IsLocked="True"/>

    </Grid>
</RSISynoptic>