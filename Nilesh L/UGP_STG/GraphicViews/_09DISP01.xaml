<UserControl x:Class="UGP_EDRF._09DISP01" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
             xmlns:local="clr-namespace:UGP_EDRF" 
             mc:Ignorable="d"> 
	<Grid>
		<!--  Graphic Part  -->
		<local:_09DISP01_Translation x:Name="GRAPHIC" d:IsLocked="True"  />

		<!-- Normal Graphic Layer Path -->
		<Canvas x:Name="GRAPHIC_OVERLAY" d:IsLocked="True">
		</Canvas>

		<!-- FOD Part -->
		<Canvas x:Name='FOD' Visibility="{FODBinding}">
		<!-- Start FOD -->
		<!-- Stop FOD -->
		</Canvas>

		<!-- BL Part -->
		<Canvas x:Name='BL' Visibility='{BLBinding}' d:IsLocked="True" >
		<!-- Start BL -->
		<!-- Stop BL -->
		</Canvas>

		<!-- FAILURE Part -->
		<Canvas x:Name='FAILURE' Visibility='{FAILUREBinding}' Margin="10,-10,-10,10" d:IsLocked="True" >
		<!-- Start FAILURE -->
		<!-- Stop FAILURE -->
		</Canvas>

	</Grid>
</UserControl>
