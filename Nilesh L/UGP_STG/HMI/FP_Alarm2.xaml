<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
                    xmlns:hmi="clr-namespace:UGP_STG.HMI"
                    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">

  <Style TargetType="{x:Type hmi:FP_Alarm2}">

    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type hmi:FP_Alarm2}">
          <RSIViewbox Margin="{TemplateBinding ControlMargin}" Stretch="Fill">
            <RSIGrid Background="Transparent">
              <RSICanvas Width="85.72154"
                         Height="70.38921"
                         TextElement.FontFamily="Arial"
                         TextElement.FontSize="12">
                <RSIRectangle x:Name="ACK"
                              Canvas.Left="0.278480000000002"
                              Canvas.Top="0.335100000000011"
                              Width="85.44306"
                              Height="70.05411"
                              Stroke="#FF666666"
                              StrokeThickness="1" />
                <RSIButton x:Name="Button1"
                           Canvas.Left="0.278480000000002"
                           Canvas.Top="0.335100000000011"
                           Width="85.44306"
                           Height="70.05411"
                           Background="#FF717171">
                  <RSIButton.Animations />
                </RSIButton>
                <RSIRectangle x:Name="Element35"
                              Width="85.44304"
                              Height="70.05411"
                              Fill="#FF717171"
                              Stroke="#FFFFFFFF"
                              StrokeThickness="1" />
                <RSIText x:Name="ACK_Text"
                         Canvas.Left="29.48325"
                         Canvas.Top="26.3392"
                         Width="27.00439"
                         Height="16.99219"
                         HorizontalAlignment="Center"
                         VerticalAlignment="Center"
                         Foreground="#FFFFFFFF"
                         IsHitTestVisible="False"
                         Text="ACK"
                         TextElement.FontFamily="Courier New"
                         TextElement.FontSize="11"
                         TextElement.FontWeight="Bold" />
              </RSICanvas>
            </RSIGrid>
          </RSIViewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>

  </Style>
</ResourceDictionary>