                  <RSIBorder x:Name="Line1"
                             Canvas.Left="-0.75"
                             Canvas.Top="-0.75"
                             Width="96.59999"
                             Height="9.999996">
                    <RSIPath Data="M95.85,9.249996 L-0.75,-0.75"
                             Stretch="Fill"
                             Stroke="#FFFF0000"
                             StrokeThickness="1" />
                  </RSIBorder>
                  <RSIBorder x:Name="Line2"
                             Canvas.Left="95.84999"
                             Canvas.Top="-0.75"
                             Width="96.59998"
                             Height="9.999996"
                             RenderTransformOrigin="0.00345065633899027,0.033333346666672">
                    <RSIPath Data="M-0.74999,9.249996 L95.85,-0.75"
                             RenderTransformOrigin="0.5,0.5"
                             Stretch="Fill"
                             Stroke="#FFFF0000"
                             StrokeThickness="1">
                      <RSIPath.LayoutTransform>
                        <TransformGroup>
                          <RotateTransform Angle="-180" />
                          <ScaleTransform ScaleX="1" ScaleY="-1" />
                        </TransformGroup>
                      </RSIPath.LayoutTransform>
                    </RSIPath>
                    <RSIBorder.RenderTransform>
                      <TransformGroup>
                        <RotateTransform Angle="180" />
                        <ScaleTransform ScaleX="1" ScaleY="-1" />
                      </TransformGroup>
                    </RSIBorder.RenderTransform>
                  </RSIBorder>
                </RSICanvas>
              </RSICanvas>
            </RSIGrid>
          </RSIViewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>

