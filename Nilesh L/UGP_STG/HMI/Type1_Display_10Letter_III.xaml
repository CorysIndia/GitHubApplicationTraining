                      <GradientStop Offset="1" Color="#00FFFFFF" />
                    </LinearGradientBrush>
                  </RSIRectangle.Fill>
                  <RSIRectangle.Animations>
                    <Animation Condition="Value==true">
                      <SetVisible />
                    </Animation>
                    <Animation Condition="Value==false">
                      <Invisible />
                    </Animation>
                    <Animation Condition="Value">
                      <ChangeColor Color="" />
                    </Animation>
                  </RSIRectangle.Animations>
