                         VerticalAlignment="Center"
                         Foreground="#FF000000"
                         Text="XXXXXXXXXX"
                         TextElement.FontFamily="Arial"
                         TextElement.FontSize="10"
                         TextElement.FontWeight="Bold">
                  <RSIText.Animations>
                    <Animation Condition="Value">
                      <ChangeColor Color="#FF000000" />
                    </Animation>
                  </RSIText.Animations>
                </RSIText>
