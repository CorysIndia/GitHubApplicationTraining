<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
                    xmlns:hmi="clr-namespace:UGP_STG.HMI"
                    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">

  <Style TargetType="{x:Type hmi:NAV_RECENT}">

    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type hmi:NAV_RECENT}">
          <RSIViewbox Margin="{TemplateBinding ControlMargin}" Stretch="Fill">
            <RSIGrid Background="Transparent">
              <RSICanvas Width="55"
                         Height="30"
                         TextElement.FontFamily="Arial"
                         TextElement.FontSize="12">
                <RSICanvas Name="RECENTLY_OPEN_DISPLAY"
                           Width="55"
                           Height="30"
                           Left="172"
                           Top="171">
                  <RSIRectangle x:Name="RoundRectangle3"
                                Width="55"
                                Height="30"
                                Stroke="#FF000000"
                                StrokeThickness="1">
                    <RSIRectangle.Fill>
                      <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                        <GradientStop Offset="0" Color="#FFFFFFFF" />
                        <GradientStop Offset="0" Color="#FFB3B3B3" />
                        <GradientStop Offset="1" Color="#FFB3B3B3" />
                      </LinearGradientBrush>
                    </RSIRectangle.Fill>
                  </RSIRectangle>
                  <RSIRectangle x:Name="RoundRectangle6"
                                Canvas.Left="2.07429999999999"
                                Canvas.Top="2.63999999999999"
                                Width="50"
                                Height="24.71997"
                                Stroke="#FF7E7E7E"
                                StrokeThickness="1">
                    <RSIRectangle.Fill>
                      <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                        <GradientStop Offset="0" Color="#FFFFFFFF" />
                        <GradientStop Offset="0" Color="#FFCECECE" />
                        <GradientStop Offset="1" Color="#FFCECECE" />
                      </LinearGradientBrush>
                    </RSIRectangle.Fill>
                  </RSIRectangle>
                  <RSIPath x:Name="Polygon5"
                           Canvas.Left="21.10161"
                           Canvas.Top="24.8609"
                           Width="19.86011"
                           Height="12"
                           Data="M0,1 L0.5,0 1,1 0,1 z"
                           Fill="#FFFFFFFF"
                           RenderTransformOrigin="0.0251760941908177,0.0416666666666667"
                           Stretch="Fill"
                           Stroke="Transparent"
                           StrokeThickness="1">
                    <RSIPath.RenderTransform>
                      <RotateTransform Angle="-90" />
                    </RSIPath.RenderTransform>
                  </RSIPath>
                  <RSIPath x:Name="Polygon6"
                           Canvas.Left="21.17642"
                           Canvas.Top="24.3309"
                           Width="18.00009"
                           Height="11.00003"
                           Data="M0,1 L0.5,0 1,1 0,1 z"
                           Fill="#FF000000"
                           RenderTransformOrigin="0.0277776388895833,0.0454544214879414"
                           Stretch="Fill"
                           Stroke="Transparent"
                           StrokeThickness="1">
                    <RSIPath.RenderTransform>
                      <RotateTransform Angle="-90" />
                    </RSIPath.RenderTransform>
                  </RSIPath>
                </RSICanvas>
              </RSICanvas>
            </RSIGrid>
          </RSIViewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>

  </Style>
</ResourceDictionary>