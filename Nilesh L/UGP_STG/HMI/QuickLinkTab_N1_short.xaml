                              Width="160"
                              Height="1"
                              Fill="#FF000000"
                              Stroke="#FF000000"
                              StrokeThickness="0">
                  <RSIRectangle.Animations>
                    <Animation Condition="_Not_Selected__And_TabVisible==true">
                      <SetVisible />
                    </Animation>
                    <Animation Condition="_Not_Selected__And_TabVisible==false">
                      <Invisible />
                    </Animation>
                  </RSIRectangle.Animations>
                </RSIRectangle>
                <RSIRectangle x:Name="BottomBorder"
                              Canvas.Top="23"
                              Width="162"
                              Height="4"
                              Fill="#FFCECECE"
