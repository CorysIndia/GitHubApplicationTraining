                             Stretch="Fill"
                             Stroke="#FF000000"
                             StrokeThickness="1" />
                  </RSIBorder>
                </RSICanvas>
                <RSICanvas Name="Path2"
                           Width="154"
                           Height="21"
                           IsHitTestVisible="false"
                           Left="1"
                           Top="1">
                  <RSIPath x:Name="Arc1_2"
                           Canvas.Top="-1.99999999992428E-05"
                           Width="6.089283"
                           Height="2.89819"
                           Data="M 0,1 A 0.5,0.5 90 0 1 1,1"
                           Stretch="Fill"
                           Stroke="#FF000000"
                           StrokeThickness="1" />
                  <RSIPath x:Name="Arc2_2"
                           Canvas.Left="154"
                           Canvas.Top="-1.99999999992428E-05"
                           Width="6.089279"
                           Height="2.89819"
                           Data="M 0,1 A 0.5,0.5 90 0 1 1,1"
                           RenderTransformOrigin="0.0821115274895435,0.172521470296978"
                           Stretch="Fill"
                           Stroke="#FF000000"
                           StrokeThickness="1">
                    <RSIPath.RenderTransform>
                      <TransformGroup>
                        <RotateTransform Angle="180" />
                        <ScaleTransform ScaleX="1" ScaleY="-1" />
                      </TransformGroup>
                    </RSIPath.RenderTransform>
                  </RSIPath>
                  <RSIBorder x:Name="Line1_2"
                             Canvas.Left="-0.75"
                             Canvas.Top="20.24998"
                             Width="154"
                             Height="1.5">
                    <RSIPath Data="M-0.75,20.24998 L153.25,20.24998"
                             Stretch="Fill"
                             Stroke="#FF000000"
                             StrokeThickness="1" />
                  </RSIBorder>
                </RSICanvas>
                <RSIText x:Name="Caption"
                         Canvas.Left="65.9458"
                         Canvas.Top="4.58398"
                         Width="30.3418"
                         Height="14.52344"
                         HorizontalAlignment="Center"
                         VerticalAlignment="Center"
                         Foreground="#FF000000"
                         Text="Tab1"
                         TextElement.FontFamily="Arial"
                         TextElement.FontSize="10"
                         TextElement.FontWeight="Bold">
                  <RSIText.Animations>
                    <Animation Condition="TabVisible==true">
                      <SetVisible />
                    </Animation>
                    <Animation Condition="TabVisible==false">
                      <Invisible />
                    </Animation>
                    <Animation Condition="Selected">
                      <ChangeColor Color="#FFFFFFFF" />
                    </Animation>
                  </RSIText.Animations>
                </RSIText>
              </RSICanvas>
            </RSIGrid>
          </RSIViewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>

  </Style>
</ResourceDictionary>