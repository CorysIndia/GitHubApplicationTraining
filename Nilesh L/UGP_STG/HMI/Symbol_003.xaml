                    <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                      <GradientStop Offset="0.5" Color="#FFFFFFFF" />
                      <GradientStop Offset="0" Color="#FF9A9A9A" />
                      <GradientStop Offset="1" Color="#FF9A9A9A" />
                    </LinearGradientBrush>
                  </RSIRectangle.Fill>
                </RSIRectangle>
                <RSIRectangle Canvas.Left="-80"
                              Canvas.Top="-42"
                              Width="86"
                              Height="95.333"
                              Fill="#00000000" />
                <RSICanvas Name="Path1"
                           Width="95"
                           Height="85"
                           IsHitTestVisible="false"
                           Left="675"
                           Top="415">
                  <RSIPath x:Name="PolyLine1"
                           Canvas.Left="-755.5"
                           Canvas.Top="-362.4999"
                           Width="96.00012"
