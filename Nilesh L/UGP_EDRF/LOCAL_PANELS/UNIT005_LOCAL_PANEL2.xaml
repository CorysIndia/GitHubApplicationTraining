        <!--  FOD Part  -->
        <Canvas x:Name="FOD"
                Visibility="{FODBinding}">
            <RSI_StdPushButton Canvas.Left="782.5" Canvas.Top="262.037" ModelName="05ZHS0701A" StartText="ON" StopText="OFF" ParameterName="COUT" DescriptionText="BUTTON" RenderTransformOrigin="0.4,0.613"/>
            <RSI_StdPushButton Canvas.Left="782.5" Canvas.Top="459.037" ModelName="05ZHS0701B" StartText="ON" StopText="OFF" ParameterName="COUT" DescriptionText="BUTTON" RenderTransformOrigin="0.4,0.613"/>
