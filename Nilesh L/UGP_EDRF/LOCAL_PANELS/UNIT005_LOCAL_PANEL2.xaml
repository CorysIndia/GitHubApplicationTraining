            <TextBlock
                FontSize="48"
                Canvas.Left="291" Canvas.Top="169.504" Height="86.533" Width="1619" FontWeight="Bold" Text="LOCAL ESD ACTUATING BUTTON M39-K-0003B" />
            <TextBlock
                FontSize="24"
                Canvas.Left="746.167" Canvas.Top="327.471" Height="41.033" Width="148.04" FontWeight="Bold" Text="05ZHS0701A" />
            <TextBlock
                FontSize="48"
                Canvas.Left="291" Canvas.Top="383.504" Height="86.533" Width="1619" FontWeight="Bold" Text="SAFE-LOCATION ESD ACTUATING BUTTON M39-K-0003B" />
