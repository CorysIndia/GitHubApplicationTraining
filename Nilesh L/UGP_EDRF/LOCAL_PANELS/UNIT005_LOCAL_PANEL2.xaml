        Canvas.Left="72.5"
        Canvas.Top="999.217"
        Width="29"
        Height="38.783"
        DISP="UNIT005_LOCAL_PANEL1"
        PRT="3"
        Type="Main" RenderTransformOrigin="0.5,0.5" >
                <HMI:Navigaton.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleY="1" ScaleX="-1"/>
                        <SkewTransform AngleY="0" AngleX="0"/>
                        <RotateTransform Angle="0"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Navigaton.RenderTransform>
            </HMI:Navigaton>
