            <RSI_StdPushButton Canvas.Left="782.5" Canvas.Top="891.853" ModelName="05XS3150" StartText="ON" StopText="OFF" ParameterName="COUT" DescriptionText="BUTTON" RenderTransformOrigin="0.4,0.613"/>
        </Canvas>

        <!--  Battery Limit Part  -->
        <Canvas x:Name="BL"
                Visibility="{BLBinding}" d:IsLocked="True"/>

        <!--  FAILURE Part  -->
        <Canvas x:Name="FAILURE"
                Visibility="{FAILUREBinding}" d:IsLocked="True"/>

    </Grid>
</RSISynoptic>