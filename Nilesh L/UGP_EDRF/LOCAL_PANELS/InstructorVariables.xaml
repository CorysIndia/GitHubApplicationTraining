<RSIMimic
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:PetroRabigh_CDU.LocalPanel"
    mc:Ignorable="d"
	x:Class="PetroRabigh_CDU.LocalPanel.InstructorVariables"
    Width="500" 
	Height="130"
    Background="LightGray">
    <RSICanvas  >
        <TextBox Height="30" Canvas.Left="10" TextWrapping="Wrap" Text="Instructor Variables" Canvas.Top="10" Width="480" AllowDrop="False" Focusable="False" IsHitTestVisible="False" Background="{x:Null}" BorderBrush="Black" FontSize="18.667" VerticalAlignment="Center" HorizontalAlignment="Center" TextAlignment="Center"/>
        <GroupBox Header="Ambient Temperature" Canvas.Left="10" Canvas.Top="50" Height="70">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Width="470" Height="40">
                <local:InstructorParameter Canvas.Left="10" Canvas.Top="7.5">
                    <local:InstructorParameter.Parameters>
                        <Param Name="$PROG" Value="InstructorVariable01"/>
                    </local:InstructorParameter.Parameters>
                </local:InstructorParameter>
            </Canvas>
        </GroupBox>
    </RSICanvas>
</RSIMimic>