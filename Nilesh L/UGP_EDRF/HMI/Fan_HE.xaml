<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
                    xmlns:hmi="clr-namespace:UGP_EDRF.HMI"
                    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">

  <Style TargetType="{x:Type hmi:Fan_HE}">

    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type hmi:Fan_HE}">
          <RSIViewbox Margin="{TemplateBinding ControlMargin}" Stretch="Fill">
            <RSIGrid Background="Transparent">
              <RSICanvas Width="160.6"
                         Height="129.6"
                         TextElement.FontFamily="Arial"
                         TextElement.FontSize="12">
                <RSIBorder x:Name="Line1"
                           Canvas.Left="79.65"
                           Canvas.Top="12.85"
                           Width="1.5"
                           Height="75">
                  <RSIPath Data="M79.65,12.85 L79.65,87.85"
                           Stretch="Fill"
                           Stroke="#FF000000"
                           StrokeThickness="2" />
                </RSIBorder>
                <RSIRectangle x:Name="Rectangle1"
                              Canvas.Left="5.39999999999998"
                              Canvas.Top="59.6"
                              Width="150"
                              Height="70"
                              Stroke="#FF000000"
                              StrokeThickness="2">
                  <RSIRectangle.Fill>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                      <GradientStop Offset="0.5" Color="#FF7E7E7E" />
                      <GradientStop Offset="0" Color="#FF9A9A9A" />
                      <GradientStop Offset="1" Color="#FF9A9A9A" />
                    </LinearGradientBrush>
                  </RSIRectangle.Fill>
                </RSIRectangle>
                <RSIPath x:Name="PolyLine2"
                         Canvas.Left="5.59999999999997"
                         Canvas.Top="69.6"
                         Width="149"
                         Height="52"
                         Data="M0,1 L0.1666667,0 0.3333333,1 0.5,0 0.6666667,1 0.8333333,0 1,1 "
                         Stretch="Fill"
                         Stroke="#FFFFFFFF"
                         StrokeThickness="2" />
                <RSIEllipse x:Name="Ellipse1"
                            Width="80"
                            Height="30"
                            Stroke="#FF000000"
                            StrokeThickness="2" />
                <RSIEllipse x:Name="Ellipse2"
                            Canvas.Left="80.6"
                            Width="80"
                            Height="30"
                            Stroke="#FF000000"
                            StrokeThickness="2" />
              </RSICanvas>
            </RSIGrid>
          </RSIViewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>

  </Style>
</ResourceDictionary>