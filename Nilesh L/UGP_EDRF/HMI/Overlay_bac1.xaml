<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
                    xmlns:hmi="clr-namespace:UGP_EDRF.HMI"
                    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">

  <Style TargetType="{x:Type hmi:Overlay_bac1}">

    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type hmi:Overlay_bac1}">
          <RSIViewbox Margin="{TemplateBinding ControlMargin}" Stretch="Fill">
            <RSIGrid Background="Transparent">
              <RSICanvas Width="290"
                         Height="280"
                         TextElement.FontFamily="Arial"
                         TextElement.FontSize="12">
                <hmi:Background_Disp x:Name="Background_Disp1"
                                     Width="290"
                                     Height="280" />
                <RSICanvas Name="Path1"
                           Width="43.24365"
                           Height="40.29654"
                           IsHitTestVisible="false"
                           Left="230"
                           Top="230">
                  <RSIPath x:Name="PolyLine1"
                           Canvas.Left="-0.5"
                           Canvas.Top="-0.5"
                           Width="44.24365"
                           Height="41.29654"
                           Data="M0.8,0 L0.2,0 0,0.3333333 0,0.6666667 0.2,1 0.8,1 1,0.6666667 1,0.3333333 0.8,0 "
                           Fill="#FFFFFFFF"
                           Stretch="Fill"
                           Stroke="#FF000000"
                           StrokeThickness="1" />
                </RSICanvas>
              </RSICanvas>
            </RSIGrid>
          </RSIViewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>

  </Style>
</ResourceDictionary>