<UserControl 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
             xmlns:local="clr-namespace:UGP_EDRF" 
             xmlns:HMI="clr-namespace:UGP_EDRF.HMI" x:Class="UGP_EDRF._01Disp03" 
             mc:Ignorable="d">
    <Grid>
        <!--  Graphic Part  -->
        <local:_01Disp03_Translation x:Name="GRAPHIC" d:IsLocked="True"  />

        <!-- Normal Graphic Layer Path -->
        <Canvas x:Name="GRAPHIC_OVERLAY" d:IsLocked="True"/>

        <!-- FOD Part -->
        <Canvas x:Name='FOD' Visibility="{FODBinding}" d:IsLocked="True">
            <RSI_PressureSafetyValve Canvas.Left="630" Style="{DynamicResource UGP_PressureSafetyValveStyle}" Canvas.Top="559.5" ModelName="M39_001PZV0021A" d:LayoutOverrides="Height"/>
            <!-- Start FOD -->
            <HMI:Pipeline x:Name="EmbeddedSymbol11_Copy3"
        Height="3"
        Line="Main"
        RenderTransformOrigin="0,0" Canvas.Left="580.943" Canvas.Top="664.642" Width="127.106">
                <HMI:Pipeline.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Pipeline.RenderTransform>
            </HMI:Pipeline>
            <!-- Stop FOD -->
            <HMI:Pipeline x:Name="EmbeddedSymbol11_Copy4"
        Height="3"
        Line="Main"
        RenderTransformOrigin="0,0" Canvas.Left="640.777" Canvas.Top="664.308" Width="55.272">
                <HMI:Pipeline.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Pipeline.RenderTransform>
            </HMI:Pipeline>
            <HMI:Pipeline x:Name="EmbeddedSymbol11_Copy5"
        Height="3"
        Line="Main"
        RenderTransformOrigin="0,0" Canvas.Left="652.694" Canvas.Top="597.141" Width="36.105"/>
            <RSIText x:Name="TextBox3_Copy2"
        Canvas.Left="684.437"
        Canvas.Top="566.957"
        Width="51.25"
        Height="16.75"
        VerticalContentAlignment="Center"
        BorderBrush="Transparent"
        Foreground="#FF000000"
        IsHitTestVisible="False"
        Text="TO FLARE"
        TextElement.FontFamily="Arial"
        TextElement.FontSize="10" />
            <RSI_PressureSafetyValve Canvas.Left="752.893" Style="{DynamicResource UGP_PressureSafetyValveStyle}" Canvas.Top="584.191" ModelName="M39_001PZV0021B" d:LayoutOverrides="Height"/>
            <HMI:Pipeline x:Name="EmbeddedSymbol11_Copy"
                Height="3"
                Line="Main"
                RenderTransformOrigin="0,0" Canvas.Left="775.587" Canvas.Top="621.141" Width="36.105"/>
            <RSIText x:Name="TextBox3_Copy"
                Canvas.Left="830.12"
                Canvas.Top="609.299"
                Width="51.25"
                Height="16.75"
                VerticalContentAlignment="Center"
                BorderBrush="Transparent"
                Foreground="#FF000000"
                IsHitTestVisible="False"
                Text="TO FLARE"
                TextElement.FontFamily="Arial"
                TextElement.FontSize="10" />
            <HMI:Pipeline x:Name="EmbeddedSymbol11_Copy1"
                Height="3"
                Line="Main"
                RenderTransformOrigin="0,0" Canvas.Left="764.027" Canvas.Top="665.558" Width="31.272">
                <HMI:Pipeline.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Pipeline.RenderTransform>
            </HMI:Pipeline>
            <HMI:Pipeline x:Name="EmbeddedSymbol11_Copy2"
                Height="3"
                Line="Main"
                RenderTransformOrigin="0,0" Canvas.Left="581.027" Canvas.Top="663.557" Width="184.939"/>
            <HMI:Arrow x:Name="EmbeddedSymbol13_Copy2"
                Canvas.Left="702.646"
                Canvas.Top="588.712"
                Width="17.5"
                Height="22.5"
                RenderTransformOrigin="0,0"
                Type="Main">
                <HMI:Arrow.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Arrow.RenderTransform>
            </HMI:Arrow>
            <HMI:Arrow x:Name="EmbeddedSymbol13_Copy"
                Canvas.Left="820.896"
                Canvas.Top="613.212"
                Width="17.5"
                Height="22.5"
                RenderTransformOrigin="0,0"
                Type="Main">
                <HMI:Arrow.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </HMI:Arrow.RenderTransform>
            </HMI:Arrow>
        </Canvas>

        <!-- BL Part -->
        <Canvas x:Name='BL' Visibility='{BLBinding}' d:IsLocked="True" >
            <RSI_StdBatteryLimit Canvas.Left="16.666" Canvas.Top="499.833" ModelName="BLI_UGPETHANERECOVERY_03"/>
            <RSI_StdBatteryLimit Canvas.Left="16.666" Canvas.Top="36.665" ModelName="BLO_UGPETHANERECOVERY_03"/>
            <RSI_StdBatteryLimit Canvas.Left="16.666" Canvas.Top="290.331" ModelName="BLO_UGPETHANERECOVERY_04"/>
            <RSI_StdBatteryLimit Canvas.Left="16.666" Canvas.Top="337.665" ModelName="BLO_UGPETHANERECOVERY_04"/>
            <RSI_StdBatteryLimit Canvas.Left="16.666" Canvas.Top="376.331" ModelName="BLO_UGPETHANERECOVERY_04"/>
            <RSI_StdBatteryLimit Canvas.Left="16.666" Canvas.Top="425.665" ModelName="BLO_UGPETHANERECOVERY_04"/>
            <!-- Start BL -->
            <!-- Stop BL -->
        </Canvas>

        <!-- FAILURE Part -->
        <Canvas x:Name='FAILURE' Visibility='{FAILUREBinding}' >
            <RSI_StdTransmitter Canvas.Left="297.75" Style="{DynamicResource RSI_StdTransmitterFailure_UGP}" Canvas.Top="137.75" ModelName="M39_001PDI0010" FailureFaceplateName=""/>
            <RSI_StdTransmitter Canvas.Left="297.75" Style="{DynamicResource RSI_StdTransmitterFailure_UGP}" Canvas.Top="388.917" ModelName="M39_001PDIT0173" FailureFaceplateName=""/>
            <RSI_StdTransmitter Canvas.Left="297.75" Style="{DynamicResource RSI_StdTransmitterFailure_UGP}" Canvas.Top="602.917" ModelName="M39_001PDIT0172" FailureFaceplateName=""/>
            <!-- Start FAILURE -->
            <!-- Stop FAILURE -->
        </Canvas>

    </Grid>
</UserControl>
