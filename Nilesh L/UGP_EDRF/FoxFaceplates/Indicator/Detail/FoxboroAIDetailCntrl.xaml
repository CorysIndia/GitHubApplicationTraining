<RSIFacePlate
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="UGP_EDRF.FoxFaceplates.Indicator.Detail.FoxboroAIDetailCntrl">
    <RSICanvas Height="270">
        <Rectangle Width="855"
                      Height="270"
                      Fill="#858585"
                      Stroke="#000000"
                      StrokeThickness="1" />
        <Rectangle Canvas.Left="475"
                      Canvas.Top="15"
                      Width="195"
                      Height="245"
                      Fill="#858585"
                      Stroke="#FFFFFF"
                      StrokeThickness="1" />
        <RSIText Canvas.Left="630"
                 Canvas.Top="140"
                 Width="20"
                 Height="15"
                 HorizontalContentAlignment="Center"
                 VerticalContentAlignment="Center"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#000000">
            <RSIText.Animations>
                <Animation Condition="$TAGNAME.ext_out_state == 1">
                    <ChangeText Text="T" />
                </Animation>
                <Animation Condition="$TAGNAME.ext_out_state == 2">
                    <ChangeText Text="H" />
                </Animation>
            </RSIText.Animations>
        </RSIText>
        <TextBlock Canvas.Left="490"
                 Canvas.Top="32"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="RAWC" />
        <RSIText Canvas.Left="550"
                 Canvas.Top="30"
                 Width="110"
                 Height="15"
                 HorizontalContentAlignment="Right"
                 VerticalContentAlignment="Center"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#FFFFFF"
                 Text="\$TAGNAME.RAWC:#\">
            <RSIText.Animations>
                <Animation Condition="#input == 1">
                    <ChangePropertyValue PropertyName="BorderBrush" Value="Yellow"/>
                    <ChangePropertyValue PropertyName="BorderThickness" Value="2"/>
                </Animation>
                <ClickAnimation>
                    <Assignment Expression="1" Variable="#input" />
                </ClickAnimation>
            </RSIText.Animations>
        </RSIText>
        <TextBlock Canvas.Left="490"
                 Canvas.Top="72"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="MEAS" />
        <RSIText Canvas.Left="550"
                 Canvas.Top="70"
                 Width="110"
                 Height="15"
                 HorizontalContentAlignment="Right"
                 VerticalContentAlignment="Center"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#FFFFFF"
                 Text="\$TAGNAME.MEAS:#.##\">
            <RSIText.Animations>
                <Animation Condition="#input == 2">
                    <ChangePropertyValue PropertyName="BorderBrush" Value="Yellow"/>
                    <ChangePropertyValue PropertyName="BorderThickness" Value="2"/>
                </Animation>
                <ClickAnimation>
                    <Assignment Expression="2" Variable="#input" />
                </ClickAnimation>
            </RSIText.Animations>
        </RSIText>
        <TextBlock Canvas.Left="490"
                 Canvas.Top="112"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="HSCALE" />
        <!--<RSIText Canvas.Left="550"
                 Canvas.Top="110"
                 Width="110"
                 Height="15"
                 HorizontalContentAlignment="Right"
                 VerticalContentAlignment="Center"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="\$TAGNAME.HSCALE:#.##\" />-->
        <TextBlock Canvas.Left="490"
                 Canvas.Top="152"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="LSCALE" />
        <!--<RSIText Canvas.Left="550"
                 Canvas.Top="150"
                 Width="110"
                 Height="15"
                 HorizontalContentAlignment="Right"
                 VerticalContentAlignment="Center"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="\$TAGNAME.LSCALE:#.####\" />-->
        <TextBlock Canvas.Left="490"
                 Canvas.Top="222"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="OSV" />
        <RSIText Canvas.Left="550"
                 Canvas.Top="220"
                 Width="110"
                 Height="15"
                 HorizontalContentAlignment="Right"
                 VerticalContentAlignment="Center"
                 Background="#858585"
                 FontFamily="Fixedsys"
                 FontSize="10.667"
                 Foreground="#C8C8C8"
                 Text="\$TAGNAME.OSV:#.#\" />
    </RSICanvas>
</RSIFacePlate>