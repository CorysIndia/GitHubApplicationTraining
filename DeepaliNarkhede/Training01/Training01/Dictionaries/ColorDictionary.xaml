<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="Black">Black</Color>
	<Color x:Key="Red">Red</Color>
	<Color x:Key="Dark Red">Maroon</Color>
	<Color x:Key="Green">Lime</Color>
	<Color x:Key="Dark Green">Green</Color>
	<Color x:Key="Blue">Blue</Color>
	<Color x:Key="Dark Blue">Navy</Color>
	<Color x:Key="Yellow">Yellow</Color>
	<Color x:Key="Dark Yellow">Olive</Color>
	<Color x:Key="Magenta">Magenta</Color>
	<Color x:Key="Dark Magenta">Purple</Color>
	<Color x:Key="Cyan">Cyan</Color>
	<Color x:Key="Dark Cyan">Teal</Color>
	<Color x:Key="White">White</Color>
	<Color x:Key="Light Gray">Silver</Color>
	<Color x:Key="Orange">#FFFF8000</Color>
	<Color x:Key="Deep Green">#FF003C00</Color>
	
	
	<SolidColorBrush x:Key="SynopticBackground" Color="{StaticResource Light Gray}"/>
	
	
	<LinearGradientBrush x:Key="Pipe_Black" EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="{StaticResource Black}" Offset="0"/>
			<GradientStop Color="{StaticResource Black}" Offset="1"/>
			<GradientStop Color="White" Offset="0.5"/>
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="Pipe_Red" EndPoint="0.5,1" StartPoint="0.5,0">
			<GradientStop Color="{StaticResource Red}" Offset="0"/>
			<GradientStop Color="{StaticResource Red}" Offset="1"/>
			<GradientStop Color="White" Offset="0.5"/>
	</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Dark Red" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Dark Red}" Offset="0"/>
					<GradientStop Color="{StaticResource Dark Red}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
				
	<LinearGradientBrush x:Key="Pipe_Green" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Green}" Offset="0"/>
					<GradientStop Color="{StaticResource Green}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Dark Green" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Dark Green}" Offset="0"/>
					<GradientStop Color="{StaticResource Dark Green}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
		<LinearGradientBrush x:Key="Pipe_Blue" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Blue}" Offset="0"/>
					<GradientStop Color="{StaticResource Blue}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Dark Blue" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Dark Blue}" Offset="0"/>
					<GradientStop Color="{StaticResource Dark Blue}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Yellow" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Yellow}" Offset="0"/>
					<GradientStop Color="{StaticResource Yellow}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Dark Yellow" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Dark Yellow}" Offset="0"/>
					<GradientStop Color="{StaticResource Dark Yellow}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Magenta" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Magenta}" Offset="0"/>
					<GradientStop Color="{StaticResource Magenta}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Dark Magenta" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Dark Magenta}" Offset="0"/>
					<GradientStop Color="{StaticResource Dark Magenta}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Cyan" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Cyan}" Offset="0"/>
					<GradientStop Color="{StaticResource Cyan}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Dark Cyan" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Dark Cyan}" Offset="0"/>
					<GradientStop Color="{StaticResource Dark Cyan}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_White" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource White}" Offset="0"/>
					<GradientStop Color="{StaticResource White}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<LinearGradientBrush x:Key="Pipe_Light Gray" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Light Gray}" Offset="0"/>
					<GradientStop Color="{StaticResource Light Gray}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
				
	<LinearGradientBrush x:Key="Pipe_Orange" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Orange}" Offset="0"/>
					<GradientStop Color="{StaticResource Orange}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
				
	<LinearGradientBrush x:Key="Pipe_Deep Green" EndPoint="0.5,1" StartPoint="0.5,0">
					<GradientStop Color="{StaticResource Deep Green}" Offset="0"/>
					<GradientStop Color="{StaticResource Deep Green}" Offset="1"/>
					<GradientStop Color="White" Offset="0.5"/>
				</LinearGradientBrush>
			
	<Color x:Key="ButtonLabelColor">#FF0050A0</Color>
	<LinearGradientBrush x:Key="ToolBarGradiant" EndPoint="0.5,1" StartPoint="0.5,0">
		<GradientStop Color="White" Offset="0.5"/>
		<GradientStop Color="#FF425262" Offset="1"/>
		<GradientStop Color="#FF425262"/>
	</LinearGradientBrush>
	<Style x:Key="LabelStyleBottomBar" TargetType="{x:Type Label}">
		<Setter Property="Foreground">
			<Setter.Value>
				<SolidColorBrush Color="{StaticResource ButtonLabelColor}"/>
			</Setter.Value>
		</Setter>
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Padding" Value="5"/>
		<Setter Property="HorizontalContentAlignment" Value="Left"/>
		<Setter Property="VerticalContentAlignment" Value="Top"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Label}">
					<Border SnapsToDevicePixels="true" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}">
						<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" RecognizesAccessKey="True"/>
					</Border>
					<ControlTemplate.Triggers>
						<Trigger Property="IsEnabled" Value="false">
							<Setter Property="Foreground" Value="{StaticResource {x:Static SystemColors.GrayTextBrushKey}}"/>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>