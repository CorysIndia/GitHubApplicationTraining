<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">
    <DataTemplate x:Key="TOC">
        <TreeView >
            <TreeViewItem FontSize="14" Header="Views">
                <TreeViewItem FontSize="12" Header="Home">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="HomeScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="Overview">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="OverviewScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="Process">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="ProcessScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="ESD">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="ESDScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
            </TreeViewItem>
            <TreeViewItem FontSize="14" Header="Specific">
                <TreeViewItem FontSize="12" Header="Trend">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="TrendScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="Control Group">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="ControlGroupScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="Control Scheme">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="ControlSchemeScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="Profile">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="ProfileScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
                <TreeViewItem FontSize="12" Header="Report">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="MouseUp">
                            <LinkTriggerAction WindowName="ReportScreen" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                </TreeViewItem>
            </TreeViewItem>
        </TreeView>
    </DataTemplate>

</ResourceDictionary>